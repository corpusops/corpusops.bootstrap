- hosts: all
  vars:
    checkouts:
      - repo: '__CORPUSOPS_URL__'
        dest: '{{prefix}}'
        version: '__CORPUSOPS_BRANCH__'
  tasks:
    - include: './checkouts_tasks.yml'
      with_items: "{{ checkouts }}"
      tags: checkouts,checkout
      loop_control:
        loop_var: checkout_item

