#!/usr/bin/env bash
export OW=$(pwd)
cd "$(dirname "$(readlink -f ${0})")/.."
export W=$(pwd)
sc=bin/cops_shell_common
[[ ! -e $sc ]] && echo "missing $sc" >&2
. $sc || exit 1
cd "$OW"
VERBOSE=${VERBOSE-1}
if [[ -n $VERBOSE ]];then
    vv run_silent "${@}"
else
    run_silent "${@}"
fi
ret=$?
exit $ret
# vim:set et sts=5 ts=4 tw=0:
